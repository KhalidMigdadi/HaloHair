
@{
    ViewData["Title"] = "MySchedule";
    Layout = "~/Views/Shared/_Layout_Barber.cshtml";

}

@model List<TimeSlot>

<h3>Your schedule</h3>

<table class="table">
    <thead>
        <tr>
            <th>From</th>
            <th>To</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var slot in Model)
        {
            <tr>
                <td>@slot.StartTime.ToString("g")</td>
                <td>@slot.EndTime.ToString("g")</td>
            </tr>
        }
    </tbody>
</table>

